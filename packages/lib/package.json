{
  "name": "react-global-component",
  "version": "0.0.1",
  "author": "InfiniteXyy",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://github.com/InfiniteXyy/zoov",
  "description": "Share your stateful components globally, even over internet",
  "keywords": [
    "React",
    "global-components",
    "valtio",
    "yjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/InfiniteXyy/react-global-component"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "release": "cp ../../README.md ./README.md && pnpm build && pnpm publish"
  },
  "dependencies": {
    "memoize-one": "^6.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "valtio": ">=1.0.0"
  },
  "optionalDependencies": {
    "valtio-yjs": ">=0.3.1",
    "yjs": ">=13.0.0",
    "y-websocket": ">=1.0.0",
    "y-webrtc": ">=10.0.0"
  }
}
